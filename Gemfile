# A sample Gemfile
source "https://rubygems.org"

gem 'sinatra'
gem 'redis'
gem 'bitbucket_rest_api', github: 'digitalnatives/bitbucket'
gem 'dotenv'
gem 'passenger'

group :development do
  gem 'debugger'
  gem 'guard-rspec', require: false
end

group :test do
  gem 'mock_redis'
  gem 'rspec'
  gem 'rack-test'
end
